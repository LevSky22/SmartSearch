{
    "manifest_version": 3,
    "name": "SmartSearch",
    "version": "2.0.1",
    "description": "Smart search engine that automatically chooses the best search engine based on your query",
    "author": "Lev Jampolsky",
    "homepage_url": "https://x.com/LevJampolsky",
    "permissions": [],
    "host_permissions": [
      "https://smartsearch.lev-jampolsky.workers.dev/*"
    ],
    "icons": {
      "16": "assets/icons/icon16.png",
      "48": "assets/icons/icon48.png",
      "128": "assets/icons/icon128.png"
    },
    "chrome_settings_overrides": {
      "search_provider": {
        "name": "SmartSearch",
        "keyword": "smart",
        "favicon_url": "https://smartsearch.lev-jampolsky.workers.dev/assets/icons/icon16.png",
        "search_url": "https://smartsearch.lev-jampolsky.workers.dev/search?q={searchTerms}",
        "suggest_url": "https://smartsearch.lev-jampolsky.workers.dev/suggest?q={searchTerms}",
        "image_url": "https://smartsearch.lev-jampolsky.workers.dev/search?q={searchTerms}&type=image",
        "encoding": "UTF-8",
        "is_default": true
      }
    }
}